{
    // "bitbake.pathToBitbakeFolder": "poky/bitbake",
    // "bitbake.pathToEnvScript": "poky/oe-init-build-env",
    // "bitbake.pathToBuildFolder": "build",
    // "bitbake.commandWrapper": "",
    // "bitbake.workingDirectory": ".",
    "files.exclude": {
        "**/.git/**": true,
        "**/_build/**": true,
        "**/buildhistory/**": true,
        "**/cache/**": true,
        "**/downloads/**": true,
        "**/node_modules/**": true,
        "**/oe-logs/**": true,
        "**/oe-workdir/**": true,
        "**/sstate-cache/**": true,
        "**/tmp*/**": true,
        "**/workspace/attic/**": true,
        "**/workspace/sources/**": true
    },
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/_build/**": true,
        "**/buildhistory/**": true,
        "**/cache/**": true,
        "**/downloads/**": true,
        "**/node_modules/**": true,
        "**/oe-logs/**": true,
        "**/oe-workdir/**": true,
        "**/sstate-cache/**": true,
        "**/tmp*/**": true,
        "**/workspace/attic/**": true,
        "**/workspace/sources/**": true
    },
    "python.analysis.exclude": [
        "**/_build/**",
        "**/.git/**",
        "**/buildhistory/**",
        "**/cache/**",
        "**/downloads/**",
        "**/node_modules/**",
        "**/oe-logs/**",
        "**/oe-workdir/**",
        "**/sstate-cache/**",
        "**/tmp*/**",
        "**/workspace/attic/**",
        "**/workspace/sources/**"
    ],
    // "recommendations": [
    //     "yocto-project.yocto-bitbake"
    // ],
    "python.autoComplete.extraPaths": [
        "poky/bitbake/lib",
        "poky/meta/lib",
        "${workspaceFolder}/sources/poky/bitbake/lib",
        "${workspaceFolder}/sources/poky/meta/lib"
    ],
    "python.analysis.extraPaths": [
        "poky/bitbake/lib",
        "poky/meta/lib",
        "${workspaceFolder}/sources/poky/bitbake/lib",
        "${workspaceFolder}/sources/poky/meta/lib"
    ],
    "files.associations": {
        "*.conf": "bitbake",
        "*.inc": "bitbake"
    }
}
